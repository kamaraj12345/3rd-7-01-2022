package org.dem;

public class BaseClass {

	public static void main(String[] args) {
		
		
		
		int  fact =7;
		int a=fact;
		
		
		for (int i = 1; i < fact; i++) {
			
			a = a*i;	//7=7*1=7
						//7=7*2=14
						//14=14*3=42
						//42=42*4=168
						//168=168*5=
			
			
		}
		System.out.println(a);
		
	}

	
}
